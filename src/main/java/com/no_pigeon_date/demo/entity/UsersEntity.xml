<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.no_pigeon_date.demo.entity.UsersEntity">
        <table name="users" schema="dateportable" />
        <attributes>
            <id name="uid">
                <column name="uid"/>
            </id>
            <basic name="username">
                <column name="username"/>
            </basic>
            <basic name="password">
                <column name="password"/>
            </basic>
            <basic name="sex">
                <column name="sex"/>
            </basic>
            <basic name="level">
                <column name="level"/>
            </basic>
            <basic name="exp">
                <column name="exp"/>
            </basic>
            <basic name="creditscore">
                <column name="creditscore"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
